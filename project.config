[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Tree_Ent (lazyreaper)
[access "refs/for/refs/*"]
	read = group user/Tree_Ent (lazyreaper)
	push = group user/Tree_Ent (lazyreaper)
	label-Code-Review = -2..+2 group user/Tree_Ent (lazyreaper)
	label-Verified = -1..+1 group user/Tree_Ent (lazyreaper)
	rebase = group user/Tree_Ent (lazyreaper)
	removeReviewer = group user/Tree_Ent (lazyreaper)
	submit = group user/Tree_Ent (lazyreaper)
[access "refs/heads/*"]
	read = group user/Tree_Ent (lazyreaper)
	create = group user/Tree_Ent (lazyreaper)
	push = +force group user/Tree_Ent (lazyreaper)
	pushMerge = group user/Tree_Ent (lazyreaper)
[access "refs/tags/*"]
	push = +force group user/Tree_Ent (lazyreaper)
	pushTag = group user/Tree_Ent (lazyreaper)
	pushSignedTag = group user/Tree_Ent (lazyreaper)
